@inherits TeamIdDependant

<div class="d-flex flex-column w-100 h-100">
    @for (var i = 1; i <= 13; i += 2)
    {
        var num = i;
        <div class="d-flex flex-row flex-grow-1 w-inherit">
            <ScoreButton ScoreValue="num" />
            <ScoreButton ScoreValue="num + 1" />
        </div>
    }
</div>